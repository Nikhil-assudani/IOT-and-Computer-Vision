   "source": [
    "kernel = np.ones((5, 5))\n",
    "imgDial = cv2.dilate(imgThreshold, kernel, iterations=2) # APPLYING DILATION\n",
    "imgThreshold = cv2.erode(imgDial, kernel, iterations=1)  # APPLYING EROSION"
   ]
